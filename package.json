{
  "name": "react-trte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "pnpm run lint -- --fix",
    "prettier:fix": "prettier --config ./prettier.config.cjs --write ./src/**/*.{css,ts,tsx} ",
    "prettier:check": "prettier --config ./prettier.config.cjs --check ./src/**/*.{css,ts,tsx}",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'pnpm run cypress:open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'pnpm run cypress:run'",
    "cy:open": "cypress open --e2e",
    "cy:run": "cypress run --e2e",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest run --coverage --environment jsdom",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.28.0",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.1",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "postcss-extend": "^1.0.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "postcss-reporter": "^7.0.5",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.29.3"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": "pnpm run lint",
    "./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier -w -u"
  }
}
